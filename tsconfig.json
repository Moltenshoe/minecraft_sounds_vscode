{
	"compilerOptions": {
	  "target": "ES2020",
	  "lib": ["ES2020", "dom"],
	  "module": "commonjs",
	  "outDir": "out",
	  "rootDir": "src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "moduleResolution": "node",
	  "types": ["node", "vscode"],
	  "useDefineForClassFields": false
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"]
  }
  